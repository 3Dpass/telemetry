{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "no-unused-variable": [true, {"ignore-pattern": "^_"}],
    "no-empty": false,
    "no-namespace": false,
    "no-bitwise": false,
    "interface-name": false
  }
}
